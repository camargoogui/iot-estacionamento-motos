  {
    "id": "formata_log",
    "type": "function",
    "z": "4f07eea542836687",
    "name": "Formata CSV",
    "func": "const tipo = msg.topic.includes(\"status\") ? \"status\" : \"distancia\";\nconst linha = `${new Date().toISOString()},${tipo},${msg.payload}\\n`;\nmsg.payload = linha;\nreturn msg;",
