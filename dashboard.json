  {
    "id": "combina_dados",
    "type": "function",
    "z": "4f07eea542836687",
    "name": "Combina Dados",
    "func": "// Usa contexto global para armazenar dados temporários\nconst contextKey = 'sensor_data';\nlet context = flow.get(contextKey) || {};\n\n// Identifica o tipo de dado recebido e armazena\nif (msg.topic.includes('distancia')) {\n    context.distancia = parseFloat(msg.payload) || 0;\n    context.timestamp = new Date().toISOString();\n} else if (msg.topic.includes('status')) {\n    context.status = msg.payload.toString().trim();\n    context.timestamp = new Date().toISOString();\n}\n\n// Salva no contexto\nflow.set(contextKey, context);\n\n// Verifica se temos ambos os dados\nif (context.distancia !== undefined && context.status !== undefined) {\n    // Prepara mensagem combinada\n    const combined = {\n        distancia: context.distancia,\n        status: context.status,\n        timestamp: context.timestamp,\n        id_vaga: flow.get('id_vaga') || 1\n    };\n    \n    // Limpa o contexto após usar\n    flow.set(contextKey, {});\n    \n    msg.payload = combined;\n    return msg;\n}\n\n// Se não temos ambos ainda, não retorna nada\nreturn null;",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 350,
    "y": 300,
    "wires": [["prepara_mongodb"]]
  },
  {
    "id": "prepara_mongodb",
    "type": "function",
    "z": "4f07eea542836687",
    "name": "Prepara MongoDB",
    "func": "// Prepara documento para inserção no MongoDB\nconst data = msg.payload;\n\n// Cria documento MongoDB\nmsg.payload = {\n    vaga_id: data.id_vaga || 1,\n    distancia: data.distancia,\n    status: data.status,\n    timestamp: new Date(data.timestamp)\n};\n\nreturn msg;",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 580,
    "y": 300,
    "wires": [["mongodb_insert"]]
  },
